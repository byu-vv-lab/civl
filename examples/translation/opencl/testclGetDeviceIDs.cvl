#include <civlc.h>
#include <stdio.h>
#include "setup/openCL.cvl"
void main()
{
  int err;
  int num_entries = 3;
  
  cl_device_id device_id;
  
  err = clGetDeviceIDs(NULL,
  CL_DEVICE_TYPE_GPU,
  num_entries,
  &device_id,
  NULL);
  printf("devices is %d", device_id[0]);
  $assert (err == CL_SUCCESS);
}
